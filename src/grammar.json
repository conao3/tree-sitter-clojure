{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "clojure",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_form"
      }
    },
    "_form": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "vector"
        },
        {
          "type": "SYMBOL",
          "name": "map"
        },
        {
          "type": "SYMBOL",
          "name": "set"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "character"
        },
        {
          "type": "SYMBOL",
          "name": "nil"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "keyword"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "vector": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "map": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "#"
                },
                {
                  "type": "SYMBOL",
                  "name": "keyword"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_form"
              },
              {
                "type": "SYMBOL",
                "name": "_form"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "set": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "symbol": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_namespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_symbol"
        }
      ]
    },
    "_symbol": {
      "type": "PATTERN",
      "value": "[a-zA-Z\\*\\+!\\-_'\\?<>=][a-zA-Z0-9\\*\\+!\\-_\\?'<>=]*"
    },
    "_namespace": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_symbol"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "."
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_symbol"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "/"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[+-]?"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "PATTERN",
                            "value": "[1-9][0-9]*"
                          },
                          {
                            "type": "PATTERN",
                            "value": "2r[01]+"
                          },
                          {
                            "type": "PATTERN",
                            "value": "0[0-7]+"
                          },
                          {
                            "type": "PATTERN",
                            "value": "0x[0-9A-Fa-f]+"
                          },
                          {
                            "type": "PATTERN",
                            "value": "36r[0-9A-Z]+"
                          }
                        ]
                      },
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "CHOICE",
                            "members": [
                              {
                                "type": "STRING",
                                "value": "N"
                              },
                              {
                                "type": "STRING",
                                "value": "M"
                              }
                            ]
                          },
                          {
                            "type": "BLANK"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "[0-9]*\\.[0-9]+"
                      },
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "SEQ",
                            "members": [
                              {
                                "type": "CHOICE",
                                "members": [
                                  {
                                    "type": "STRING",
                                    "value": "e"
                                  },
                                  {
                                    "type": "STRING",
                                    "value": "E"
                                  }
                                ]
                              },
                              {
                                "type": "PATTERN",
                                "value": "[+-]?"
                              },
                              {
                                "type": "PATTERN",
                                "value": "[1-9][0-9]*"
                              }
                            ]
                          },
                          {
                            "type": "BLANK"
                          }
                        ]
                      },
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "STRING",
                            "value": "M"
                          },
                          {
                            "type": "BLANK"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "PATTERN",
                    "value": "[0-9]+\\/[0-9]+"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "##Inf"
            },
            {
              "type": "STRING",
              "value": "##-Inf"
            },
            {
              "type": "STRING",
              "value": "##NaN"
            }
          ]
        }
      ]
    },
    "character": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "\\newline"
        },
        {
          "type": "STRING",
          "value": "\\space"
        },
        {
          "type": "STRING",
          "value": "\\tab"
        },
        {
          "type": "STRING",
          "value": "\\formfeed"
        },
        {
          "type": "STRING",
          "value": "\\backspace"
        },
        {
          "type": "STRING",
          "value": "\\return"
        },
        {
          "type": "PATTERN",
          "value": "\\\\u[0-9a-fA-F]{4}"
        },
        {
          "type": "PATTERN",
          "value": "\\\\o[0-7]{3}"
        },
        {
          "type": "PATTERN",
          "value": "\\\\."
        }
      ]
    },
    "nil": {
      "type": "STRING",
      "value": "nil"
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "keyword": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "::?"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_namespace"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_symbol"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": ";.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "STRING",
      "value": ","
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
